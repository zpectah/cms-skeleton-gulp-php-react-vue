	"meta": {
		"robots": ["index,follow","noindex,nofollow","index,nofollow","noindex,follow"]
	},
	"uploads": {
		"audio": {
			"extension": ["mp3", "aac"]
		},
		"video": {
			"extension": ["mpeg", "mp4"]
		},
		"document": {
			"extension": ["doc", "docs", "xls", "xlsx", "ppt", "pptx", "pps", "ppsx", "pages", "numbers", "pdf"]
		},
		"archive": {
			"extension": ["zip", "rar"]
		},
		"image": {
			"extension": ["jpg", "jpeg", "png"],
			"format": [
				{
					"key": "thumbnail",
					"width": 96,
					"height": 96,
					"quality": 40,
					"crop": true
				},
				{
					"key": "medium",
					"width": 960,
					"height": 960,
					"quality": 75,
					"crop": true
				},
				{
					"key": "large",
					"width": 1680,
					"height": 1680,
					"quality": 100,
					"crop": false
				},
				{
					"key": "custom_1",
					"width": 1920,
					"height": 640,
					"quality": 75,
					"crop": true
				}
			]
		}
	},
	"model": {
		"Users": {
			"level_list": ["demo","redactor","chief_redactor","admin","super_admin"],
			"group_list": ["default","redactors","managers"]
		},
		"Posts": {
			"type_list": ["article","blog","event","media","reference","attachment","custom_1"]
		},
		"Menu": {
			"type_list": ["default","primary","secondary","tertiary"]
		},
		"MenuItems": {
			"type_list": ["default","external"]
		},
		"Categories": {
			"type_list": ["default","gallery"]
		},
		"Pages": {
			"type_list": ["default","system","category","tags"]
		},
		"Uploads": {
			"type_list": ["image","audio","video","document","archive"]
		},
		"Members": {
			"group_list": ["none","members","subscribers","customers"]
		}
	}
}
